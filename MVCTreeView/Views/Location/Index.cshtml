@model List<MVCTreeView.Models.MenuModels.Location>
@{

}

<h1>Locations</h1>
<table class="table">
    <thread>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Description</th>
            <th>ParentId</th>
            <th>LocationId</th>
        </tr>
    </thread>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>@item.ParentId</td>
                <td>@item.MenuNumber</td>
                <td><a href="Location/View/@item.Id">Edit</a></td>
            </tr>
        }
    </tbody>
</table>