@model MVCTreeView.Models.AddLocations
@{

}
<h1>Add Location</h1>
<form method="post" action="Add" class="mt-5">
  <div class="mb-3">
    <label for="" class="form-label">Name</label>
    <input type="text" class="form-control" asp-for="Name"/>
  </div>
  <div class="mb-3">
    <label for="" class="form-label">Description</label>
    <input type="text" class="form-control" asp-for="Description"/>
  </div>
  <div class="mb-3">
        <label for="" class="form-label">LocationId</label>
        <input type="number" class="form-control" asp-for="MenuNumber" />
    </div>
    <div class="mb-3">
        <label for="" class="form-label">ParentId</label>
        <input type="number" class="form-control" asp-for="ParentId" />
    </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>